ponents/NavBar.js",["49","50"],"import React, { Component } from \"react\";\nimport \"../components/NavBar.css\";\n\nclass NavBar extends React.Component {\n  render() {\n    return (\n      <div className=\"App\">\n        <div class=\"nav\">\n          <div class=\"news\">\n            <span>\n              <b>News</b>\n            </span>\n          </div>\n          <p class=\"text\">\n            <marquee direction=\"left\">\n              <span>\n                • Coronavirus pandemic is going to get worse and worse and\n                worse: WHO chieft\n              </span>\n              <span>\n                &nbsp;• COVID testing required in some countries PRIOR to entry\n              </span>\n              <span>&nbsp;• More than 100.000 people have been vaccined</span>\n              <span>&nbsp;• Scotland has gone into a 1 month lockdown</span>\n            </marquee>\n          </p>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default NavBar;\n","/Users/mateoospina/Desktop/ecommerce-react/src/components/TopDestinations.js",["51"],"import React, { Component } from \"react\";\nimport \"../components/TopDestinations.css\";\n\nclass TopDestinations extends React.Component {\n  render() {\n    return (\n      <div className=\"App\" id=\"TopDestinations\">\n        <div id=\"top-destinations\" class=\"container\">\n          <h2>Top Destinations</h2>\n\n          <div class=\"child\">\n            <div class=\"box\">\n              <img class=\"img\" src=\"../assets/parisnight.jpg\" alt=\"house\" />\n              <h4>Paris, France</h4>\n              <h4 className=\"h4\">Learn More</h4>\n            </div>\n\n            <div class=\"box\">\n              <img class=\"img\" src=\"../assets/nycnighttime.jpg\" alt=\"house\" />\n              <h4>New York City, US</h4>\n              <h4 className=\"h4\">Learn More</h4>\n            </div>\n\n            <div class=\"box\">\n              <img class=\"img\" src=\"../assets/japannight.jpg\" alt=\"house\" />\n              <h4>Kyoto, Japan</h4>\n              <h4 className=\"h4\">Learn More</h4>\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default TopDestinations;\n","/Users/mateoospina/Desktop/ecommerce-react/src/components/Products.js",["52"],"import React, { Component } from \"react\";\nimport data from \"../products.json\";\nimport \"../components/Products.css\";\n\n// Creating constructor to render fresh new set of data whenever the state is mutated\n\nclass Products extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      // Grabbing products from JSON file\n\n      products: data.products,\n    };\n  }\n\n  // Function to handle filter clicks\n  // ASC = Lowest to greatest\n  // Definining compartive function, compares 2 elements together and returns the lesser of the elements\n  ascPriceHandler = () => {\n    const sortByPrice = (a, b) => a.price - b.price;\n    // grabbing the current state and then sorting based on the comparative function\n    const sortedProductsMap = [...this.state.products].sort(sortByPrice);\n    this.setState({ products: sortedProductsMap });\n  };\n\n  // Highest to low function comparative\n  descPriceHandler = () => {\n    const sortByPrice = (a, b) => b.price - a.price;\n    // grabbing the current state and then sorting based on the comparative function\n    const sortedProductsMap = [...this.state.products].sort(sortByPrice);\n    this.setState({ products: sortedProductsMap });\n  };\n\n  alphabeticalOrder = () => {\n    const sortByType = (a, b) => {\n      const countryA = a.country;\n      const countryB = b.country;\n      if (countryA < countryB) {\n        return -1;\n      }\n      if (countryA > countryB) {\n        return 1;\n      }\n      return 0;\n    };\n    const sortedProductsMap = [...this.state.products].sort(sortByType);\n    this.setState({\n      products: sortedProductsMap,\n    });\n  };\n\n  render() {\n    //   Importing JSON file as the key products\n    return (\n      <>\n        <div className=\"filter-container\">\n          <button onClick={this.alphabeticalOrder} className=\"filter-button\">\n            Filter by (Country A-Z)\n          </button>\n          <button onClick={this.ascPriceHandler} className=\"filter-button\">\n            Filter by (Lowest to Highest)\n          </button>\n\n          <button onClick={this.descPriceHandler} className=\"filter-button\">\n            Filter by (Lowest to Highest)\n          </button>\n        </div>\n        <div className=\"products-container\" id=\"Products\">\n          {this.state.products.map((product) => {\n            return (\n              <div className=\"container1\">\n                <img src={product.image}></img>\n                <h5 className=\"destination\">\n                  {product.city}, {product.country}\n                </h5>\n                <h5>{product.description}</h5>\n                <h5>{product.price}</h5>\n                <h5 className=\"button\">Learn More</h5>\n              </div>\n            );\n          })}\n        </div>\n        ;\n      </>\n    );\n  }\n}\n\nexport default Products;\n","/Users/mateoospina/Desktop/ecommerce-react/src/components/MainContent.js",["53"],"import React, { Component } from \"react\";\nimport \"../components/MainContent.css\";\n\nclass MainContent extends React.Component {\n  render() {\n    return (\n      <div className=\"App\">\n        <div className=\"hero2\">\n          Check Out Our Packages!\n          <div className=\"idk\">Flight + Hotel Included!</div>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default MainContent;\n","/Users/mateoospina/Desktop/ecommerce-react/src/components/Form.js",["54"],"import React, { Component } from \"react\";\nimport \"../components/Form.css\";\n\nclass Form extends React.Component {\n  render() {\n    return (\n      <div className=\"App\" id=\"Form\">\n        <div className=\"dots\">\n          <spa